---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
     xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd"
     xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
  <title>{{ site.title | xml_escape }}</title>
  <link>{{ site.url }}{{ site.baseurl }}/</link>
  <language>en-us</language>
  <itunes:author>Rambling Bramble Blog</itunes:author>
  <itunes:owner>
    <itunes:name>Rambling Bramble Blog</itunes:name>
    <itunes:email>rambling.bramble.blog@gmail.com</itunes:email>
  </itunes:owner>
  <itunes:type>episodic</itunes:type>
  <copyright>Copyright {{ site.time | date: "%Y" }} Rambling Bramble Blog</copyright>
  <description>{{ site.description | xml_escape }}</description>
  <itunes:summary>{{ site.description | xml_escape }}</itunes:summary>
  <itunes:explicit>yes</itunes:explicit>
  <itunes:category text="Society &amp; Culture">
    <itunes:category text="Personal Journals"/>
  </itunes:category>
  <itunes:category text="Arts">
    <itunes:category text="Performing Arts"/>
  </itunes:category>
  <itunes:category text="Philosophy"/>
  </itunes:category>
  <atom:link href="{{ site.url }}{{ site.baseurl }}/feed.xml" rel="self" type="application/rss+xml"/>
  <lastBuildDate>{{ site.time | date_to_rfc822 | xml_escape }}</lastBuildDate>
  <itunes:image href="{{ site.url }}{{ site.baseurl }}/assets/logo.jpeg" />
  
  {% assign sorted_episodes = site.episodes | sort: "date" | reverse %}
  {% for episode in sorted_episodes %}
  <item>
    <title>{{ episode.title | xml_escape }}</title>
    <itunes:author>{{ episode.itunes_author | xml_escape }}</itunes:author>
    <content:encoded><![CDATA[{{ episode.content }}]]></content:encoded>
    <description>{{ episode.content | strip_html | strip_newlines | xml_escape }}</description>
    <itunes:summary>{{ episode.itunes_summary | xml_escape }}</itunes:summary>
    <pubDate>{{ episode.date | date_to_rfc822 | xml_escape }}</pubDate>
    <guid>{{ site.url }}{{ site.baseurl }}{{ episode.url }}</guid>
    <link>{{ site.url }}{{ site.baseurl }}{{ episode.url }}</link>
    <enclosure url="{{ episode.audio_url | xml_escape }}" length="{{ episode.audio_size }}" type="{{ episode.audio_type }}" />
    <itunes:duration>{{ episode.duration }}</itunes:duration>
    <itunes:explicit>no</itunes:explicit>
  </item>
  {% endfor %}
</channel>
</rss>
